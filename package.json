  },
  "devDependencies": {
    "@cap-js/sqlite": "^1"
  },
  "scripts": {
    "start": "npx cds run"
  },
  "cds": {
    "requires": {
      "sf_api_service": {
        "kind": "odata-v2",
        "model": "srv/external/sf-api-service",
        "credentials": {
          "[production]": {
            "destination": "sf-api",
            "path": "odata/v2"
          },
          "[development]": {
            "url": "https://api22preview.sapsf.com/odata/v2"
          }
        }
      }
    }
